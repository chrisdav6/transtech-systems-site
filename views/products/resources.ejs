<% include ./partials/header %>

  <!-- Resources Section -->
  <section id="resources">
    <div class="container text-center text-md-left">
      <h2>Customer Resources</h2>
      <hr>

      <p>Thank you for your interest in TransTech Systems products. Below you will find quick start guides, product manuals and any other resources needed to get you up and running with your new product.</p>

      <h3 class="my-5">Density Gauges</h3>

      <div class="row">

        <div class="col-lg-4 col-md-6 mb-4">
          <div class="card text-center">
            <div class="card-body">
              <img class="card-img-top mb-4" src="../img/pqi380-featured.png" alt="TransTech Systems PQI 380 Non-Nuclear Asphalt Density Gauge">
              <h3 class="card-title">PQI 380</h3>
              <h5 class="mb-3">Non-Nuclear Asphalt Density Gauge</h5>
              <button type="button" class="btn btn-dark btn-block btn-lg mb-2" data-id="PQI 380 Manual" data-toggle="modal" data-target="#manualDownload">User Manual</button>
              <button type="button" class="btn btn-dark btn-block btn-lg" data-id="PQI 380 QuickStart Guide" data-toggle="modal" data-target="#manualDownload">Quick Start Guide</button>
              <button type="button" class="btn btn-dark btn-block btn-lg" data-id="PQI 380 Offset Cheat Sheet" data-toggle="modal" data-target="#manualDownload">Offset Cheat Sheet</button>
            </div>
          </div>
        </div>

        <div class="col-lg-4 col-md-6 mb-4">
          <div class="card text-center">
            <div class="card-body">
              <img class="card-img-top mb-4" src="../img/sdg200-featured.png" alt="TransTech Systems SDG 200 Non-Nuclear Soil Density Gauge">
              <h3 class="card-title">SDG 200</h3>
              <h5 class="mb-3">Non-Nuclear Soil Density Gauge</h5>
              <button type="button" class="btn btn-dark btn-block btn-lg mb-2" data-id="SDG 200 Manual" data-toggle="modal" data-target="#manualDownload">User Manual</button>
              <button type="button" class="btn btn-dark btn-block btn-lg" data-id="SDG 200 QuickStart Guide" data-toggle="modal" data-target="#manualDownload">Quick Start Guide</button>
              <button type="button" class="btn btn-dark btn-block btn-lg" data-id="SDG 200 StartUp Cheat Sheet" data-toggle="modal" data-target="#manualDownload">Start Up Guide</button>
            </div>
          </div>
        </div>

      </div><!-- /row -->

      <h3 class="my-5">Compaction Products</h3>

      <div class="row">

        <div class="col-lg-4 col-md-6 mb-4">
          <div class="card text-center">
            <div class="card-body">
              <img class="card-img-top mb-4" src="../img/nwjm-featured.png" alt="TransTech Systems Notched Wedge Joint Maker">
              <h3 class="card-title">NWJM</h3>
              <h5 class="mb-3">Notched Wedge Joint Maker Screed Attachment</h5>
              <button type="button" class="btn btn-dark btn-block btn-lg mb-2" data-id="NWJM Manual" data-toggle="modal" data-target="#manualDownload">Original User Manual</button>
              <button type="button" class="btn btn-dark btn-block btn-lg mb-2" data-id="NWJM Thin Lift Manual" data-toggle="modal" data-target="#manualDownload">Thin-Lift User Manual</button>
            </div>
          </div>
        </div>

        <div class="col-lg-4 col-md-6 mb-4">
          <div class="card text-center">
            <div class="card-body">
              <img class="card-img-top mb-4 swm" src="../img/swm-featured.png" alt="TransTech Systems Shoulder Wedge Maker">
              <h3 class="card-title">SWM</h3>
              <h5 class="mb-3">Shoulder Wedge Maker Screed Attachment</h5>
              <button type="button" class="btn btn-dark btn-block btn-lg mb-2" data-id="SWM Manual" data-toggle="modal" data-target="#manualDownload">User Manual</button>
            </div>
          </div>
        </div>

        <div class="col-lg-4 col-md-6 mb-4">
          <div class="card text-center">
            <div class="card-body">
              <img class="card-img-top mb-4" src="../img/pts3000-featured.png" alt="TransTech Systems Pavement Temperature Sentry">
              <h3 class="card-title">PTS 3000</h3>
              <h5 class="mb-3">Pavement Temperature Sentry</h5>
              <button type="button" class="btn btn-dark btn-block btn-lg mb-2" data-id="PTS 3000 Manual" data-toggle="modal" data-target="#manualDownload">User Manual</button>
            </div>
          </div>
        </div>

      </div><!-- /row -->

    </div><!-- /container -->
  </section>
  <!-- /Resources Section -->

  <% include ./partials/footer %>