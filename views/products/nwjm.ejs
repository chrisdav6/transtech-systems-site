  <!-- product images section -->
  <section id="productImages" class="py-4">
    <div class="container">
      
      <div class="row">
        <div class="col imageContainer d-flex flex-wrap justify-content-center justify-content-md-start">
          <a href="img/nwjm-1.jpg" data-lightbox="NWJM" data-title="NWJM Front View"><img src="img/nwjm-1.jpg" alt="NWJM Front View" class="img-thumbnail"></a>
          <a href="img/nwjm-2.jpg" data-lightbox="NWJM" data-title="NWJM Top View"><img src="img/nwjm-2.jpg" alt="NWJM Top View" class="img-thumbnail"></a>
