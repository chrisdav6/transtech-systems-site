      <div class="row">
        <div class="col">
          <h3 class="mb-0">Give us a call today for a quote on the <span class="yellow">NWJM!</span></h3>
        </div>
      </div>
      
    </div>
  </section>
  <!-- /product message section -->
  
  
  <!-- Modals -->
  
  <!-- Download Manual Modal -->
  
  <div class="modal fade communityModal" id="manualDownload" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h4 class="modal-title">Manual Download</h4>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <form id="manualForm" action="/products/nwjm" method="post">
            <div class="form-group d-none">
              <input type="text" class="form-control form-control-lg" name="manualName" id="manualName"> <!-- Send Manual name to backend -->
            </div>
            <div class="form-group">
              <label for="name">Name</label>
              <input type="text" class="form-control form-control-lg" name="name" id="name" required>
            </div>
            <div class="form-group">
              <label for="company">Company</label>
              <input type="text" class="form-control form-control-lg" name="company" id="company" required>
            </div>
            <div class="form-group">
              <label for="country">Country</label>
              <input type="text" class="form-control form-control-lg" name="country" id="country" required>
            </div>
            <div class="form-group">
              <label for="email">Email Address</label>
              <input type="email" class="form-control form-control-lg" name="email" id="email" required>
            </div>
            <div class="form-group mt-4">
              <label class="d-block">Current TransTech Systems Customer?</label>
              <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="currentCustomer" id="currentCustomerYes" value="Yes" checked>
                <label class="form-check-label" for="currentCustomerYes">Yes</label>
              </div>
              <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="currentCustomer" id="currentCustomerNo" value="No">
                <label class="form-check-label" for="currentCustomerNo">No</label>
              </div>
            </div>
            <div class="form-group mt-4 hearAbout">
              <label for="reach">How did you hear about us?</label>
              <select class="form-control form-control-lg" name="reach" id="reach">
                <option>Asphalt Contractor Magazine</option>
                <option>NAPA - Asphalt Pavement Magazine</option>
                <option>Asphalt Pro Magazine</option>
                <option>TXAPA Magazine</option>
                <option>Ohio Flexible Pavements Magazine</option>
                <option>Florida Asphalt Association Magazine</option>
                <option>Facebook</option>
                <option>Twitter</option>
                <option>Google Plus</option>
                <option>Instagram</option>
                <option>Linked In</option>
                <option>World of Asphalt</option>
                <option>ConExpo</option>
                <option>Other Tradeshow</option>
                <option>Friend</option>
              </select>
            </div>
            <div class="form-group d-none"><!--HoneyPot-->
              <input type="text" class="form-control form-control-lg" name="address" id="address">
            </div>
            <div class="d-flex justify-content-center">
              <button id="manualDownloadBtn" class="btn btn-success btn-lg submit-btn btn-block mt-5">Submit</button>
              <i class="fas fa-spinner fa-2x fa-spin mt-4 mb-3 mx-auto"></i><!--Initially hidden in the css-->
              <span class="submitMessage text-center mx-auto">Thank you, you may download the manual</span><!--Initially hidden in the css-->
            </div>
          </form>
          <a id="manualDownloadLink" class="btn btn-outline-success btn-lg btn-block mt-3" href="#" target="_blank">Download Manual</a><!--Initially hidden in the css-->
        </div>
      </div>
    </div>
  </div>
  
