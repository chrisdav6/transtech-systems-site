<% include ./partials/header %>
  
  <!-- map section -->
  <section id="map">
    <!-- Using google maps embed option rather than javascript to be able to point to TransTech location -->
    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d1503579.6624896529!2d-73.74447104786897!3d42.604553052073626!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x89de726b6f4a031f%3A0x49fd09a9edb49f34!2sTranstech+Systems+Inc!5e0!3m2!1sen!2sus!4v1522931800379" width="100%" height="100%" frameborder="0" style="border:0" allowfullscreen></iframe>
  </section>
  <!-- /map section -->
  
  <!-- address Section -->
  <section id="address">
    <div class="container">
      <div class="row">
        
        <div class="col-md-6 text-center text-md-left">
          <h2 class="pb-3">Corporate Office</h2>
            <div class="contact-info">
              <address>
                900 Albany Shaker Road<br>
                Suite #2<br>
                Latham, NY 12110<br>
              </address>
              
              <p><a class="phone-link" href="tel:+5183705558"><i class="fas fa-phone-square" data-fa-transform="flip-h"></i> 518-370-5558</a></p>
              <p><a class="phone-link" href="tel:+18007246306"><i class="fas fa-phone-square" data-fa-transform="flip-h"></i> 1-800-724-6306</a></p>
              <p><i class="fas fa-fax"></i> 518-370-5538</p>
              <p><a class="phone-link" href="mailto:sales@transtechsys.com?subject=Sales Request"><i class="fas fa-envelope"></i> Email Sales</a></p>
            </div>
        </div>
        
        <div class="col-md-6 mt-5 mt-md-0">
          <h2 class="pb-3 text-center text-md-left">Contact Us</h2>
          
          <form action="" method="post" class="d-flex flex-column">
            <div class="form-group">
              <label for="name" class="text-left">Name</label>
              <input type="text" class="form-control form-control-lg" name="name" id="name" required>
            </div>
            <div class="form-group">
              <label for="email">Email Address</label>
              <input type="email" class="form-control form-control-lg" name="email" id="email" required>
            </div>
            <div class="form-group">
              <label for="message">Message</label>
              <textarea class="form-control form-control-lg" name="message" id="message" rows="5" required></textarea>
            </div>
            <button class="btn btn-outline-success btn-lg submit-btn ml-0 ml-lg-auto">Submit</button>
          </form>
        </div>
      </div>
    </div>
  </section>
  <!-- /address Section -->
  
  
<% include ./partials/footer %>